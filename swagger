    "/clients/": {
      "get": {
        "operationId": "clients_list",
        "description": "A viewset for viewing and editing Client instances, with granular permissions.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "A page number within the paginated result set.",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "required": [
                "count",
                "results"
              ],
              "type": "object",
              "properties": {
                "count": {
                  "type": "integer"
                },
                "next": {
                  "type": "string",
                  "format": "uri",
                  "x-nullable": true
                },
                "previous": {
                  "type": "string",
                  "format": "uri",
                  "x-nullable": true
                },
                "results": {
                  "type": "array",
                  "items": {
                    "required": [
                      "client_name",
                      "email",
                      "phone_number"
                    ],
                    "type": "object",
                    "properties": {
                      "id": {
                        "title": "ID",
                        "type": "integer",
                        "readOnly": true
                      },
                      "client_name": {
                        "title": "Client name",
                        "type": "string",
                        "maxLength": 255,
                        "minLength": 1
                      },
                      "email": {
                        "title": "Email",
                        "type": "string",
                        "format": "email",
                        "maxLength": 254,
                        "minLength": 1
                      },
                      "phone_number": {
                        "title": "Phone number",
                        "type": "string",
                        "pattern": "^\\+?\\d{10,15}$",
                        "maxLength": 30,
                        "minLength": 1
                      },
                      "nationality": {
